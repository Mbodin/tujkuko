		"@opam/js_of_ocaml-ppx_deriving_json": "*",
		"@opam/odoc": "*",
		"@opam/ppx_inline_test": "*",
		"ocaml": "~4.7",
		"libutils": "github:Mbodin/libutils#5c210a5ad3bfd078cb27c5936cd3c2b2a6b7e8dd"
